<!-- Project Placeholder 2: ResNet-50 Training -->
<svg width="800" height="400" viewBox="0 0 800 400" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg2" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1a1a1a;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#2a2a2a;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="accent2" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#00ff88;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#00d4ff;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="800" height="400" fill="url(#bg2)"/>
  
  <!-- Neural Network Architecture -->
  <g transform="translate(100, 200)">
    <!-- Input layer -->
    <rect x="0" y="-60" width="15" height="120" rx="7" fill="#00d4ff" opacity="0.7"/>
    <text x="7" y="-70" text-anchor="middle" fill="#00d4ff" font-family="Inter, sans-serif" font-size="10">Input</text>
    
    <!-- Conv layers -->
    <rect x="50" y="-80" width="20" height="160" rx="10" fill="#00ff88" opacity="0.6"/>
    <rect x="80" y="-70" width="25" height="140" rx="12" fill="#00ff88" opacity="0.7"/>
    <rect x="115" y="-60" width="30" height="120" rx="15" fill="#00ff88" opacity="0.8"/>
    
    <!-- Residual connections -->
    <path d="M 25 -40 Q 60 -60 95 -40" stroke="#ffd700" stroke-width="2" fill="none" opacity="0.8"/>
    <path d="M 25 0 Q 60 -20 95 0" stroke="#ffd700" stroke-width="2" fill="none" opacity="0.8"/>
    <path d="M 25 40 Q 60 20 95 40" stroke="#ffd700" stroke-width="2" fill="none" opacity="0.8"/>
    
    <!-- More conv blocks -->
    <rect x="165" y="-50" width="25" height="100" rx="12" fill="#ff6b6b" opacity="0.7"/>
    <rect x="200" y="-40" width="30" height="80" rx="15" fill="#ff6b6b" opacity="0.8"/>
    <rect x="240" y="-35" width="25" height="70" rx="12" fill="#ff6b6b" opacity="0.9"/>
    
    <!-- Global Average Pooling -->
    <rect x="280" y="-20" width="15" height="40" rx="7" fill="#ffd700" opacity="0.8"/>
    <text x="287" y="-25" text-anchor="middle" fill="#ffd700" font-family="Inter, sans-serif" font-size="8">GAP</text>
    
    <!-- FC layer -->
    <rect x="310" y="-30" width="20" height="60" rx="10" fill="#00d4ff" opacity="0.9"/>
    
    <!-- Output -->
    <circle cx="350" cy="0" r="15" fill="url(#accent2)" opacity="0.9"/>
    <text x="350" y="4" text-anchor="middle" fill="white" font-family="Inter, sans-serif" font-size="10" font-weight="600">1000</text>
  </g>
  
  <!-- ImageNet samples representation -->
  <g transform="translate(500, 100)">
    <!-- Sample images as rectangles -->
    <rect x="0" y="0" width="40" height="40" rx="5" fill="#00d4ff" opacity="0.6"/>
    <rect x="45" y="0" width="40" height="40" rx="5" fill="#00ff88" opacity="0.6"/>
    <rect x="90" y="0" width="40" height="40" rx="5" fill="#ff6b6b" opacity="0.6"/>
    <rect x="0" y="45" width="40" height="40" rx="5" fill="#ffd700" opacity="0.6"/>
    <rect x="45" y="45" width="40" height="40" rx="5" fill="#00d4ff" opacity="0.4"/>
    <rect x="90" y="45" width="40" height="40" rx="5" fill="#00ff88" opacity="0.4"/>
    
    <!-- Labels -->
    <text x="65" y="105" text-anchor="middle" fill="#b3b3b3" font-family="Inter, sans-serif" font-size="12">ImageNet-1K</text>
  </g>
  
  <!-- Performance metrics -->
  <g transform="translate(500, 250)">
    <rect x="0" y="0" width="120" height="60" rx="10" fill="#2a2a2a" stroke="#00d4ff" stroke-width="1"/>
    <text x="60" y="20" text-anchor="middle" fill="#00d4ff" font-family="Inter, sans-serif" font-size="14" font-weight="600">Top-1 Accuracy</text>
    <text x="60" y="40" text-anchor="middle" fill="#00ff88" font-family="Inter, sans-serif" font-size="20" font-weight="700">76.15%</text>
    
    <rect x="130" y="0" width="120" height="60" rx="10" fill="#2a2a2a" stroke="#00ff88" stroke-width="1"/>
    <text x="190" y="20" text-anchor="middle" fill="#00ff88" font-family="Inter, sans-serif" font-size="14" font-weight="600">Top-5 Accuracy</text>
    <text x="190" y="40" text-anchor="middle" fill="#ffd700" font-family="Inter, sans-serif" font-size="20" font-weight="700">91.87%</text>
  </g>
  
  <!-- Project title -->
  <text x="400" y="50" text-anchor="middle" fill="url(#accent2)" font-family="Inter, sans-serif" font-size="32" font-weight="700">
    ResNet-50 on ImageNet-1K
  </text>
  <text x="400" y="75" text-anchor="middle" fill="#b3b3b3" font-family="Inter, sans-serif" font-size="16">
    Mixed-precision DDP Training from Scratch
  </text>
  
  <!-- Tech stack -->
  <rect x="50" y="350" width="60" height="25" rx="12" fill="#ff6b6b" opacity="0.2"/>
  <text x="80" y="367" text-anchor="middle" fill="#ff6b6b" font-family="Inter, sans-serif" font-size="11" font-weight="500">PyTorch</text>
  
  <rect x="120" y="350" width="65" height="25" rx="12" fill="#ffd700" opacity="0.2"/>
  <text x="152" y="367" text-anchor="middle" fill="#ffd700" font-family="Inter, sans-serif" font-size="11" font-weight="500">AWS EC2</text>
  
  <rect x="195" y="350" width="75" height="25" rx="12" fill="#00ff88" opacity="0.2"/>
  <text x="232" y="367" text-anchor="middle" fill="#00ff88" font-family="Inter, sans-serif" font-size="11" font-weight="500">4x A10G</text>
  
  <!-- GPU indicators -->
  <g transform="translate(650, 320)">
    <rect x="0" y="0" width="30" height="20" rx="3" fill="#00d4ff" opacity="0.8"/>
    <rect x="35" y="0" width="30" height="20" rx="3" fill="#00d4ff" opacity="0.8"/>
    <rect x="0" y="25" width="30" height="20" rx="3" fill="#00d4ff" opacity="0.8"/>
    <rect x="35" y="25" width="30" height="20" rx="3" fill="#00d4ff" opacity="0.8"/>
    <text x="32" y="60" text-anchor="middle" fill="#00d4ff" font-family="Inter, sans-serif" font-size="10">Multi-GPU</text>
  </g>
</svg>
