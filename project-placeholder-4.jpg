<!-- Project Placeholder 4: Browser Agent -->
<svg width="800" height="400" viewBox="0 0 800 400" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg4" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1a1a1a;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#2a2a2a;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="browser" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#ff6b6b;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#ffd700;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="800" height="400" fill="url(#bg4)"/>
  
  <!-- Browser Window -->
  <g transform="translate(150, 80)">
    <!-- Browser frame -->
    <rect x="0" y="0" width="500" height="300" rx="10" fill="#2a2a2a" stroke="#444" stroke-width="2"/>
    
    <!-- Browser toolbar -->
    <rect x="0" y="0" width="500" height="40" rx="10" fill="#333"/>
    <rect x="0" y="30" width="500" height="10" fill="#333"/>
    
    <!-- Browser controls -->
    <circle cx="20" cy="20" r="6" fill="#ff6b6b"/>
    <circle cx="40" cy="20" r="6" fill="#ffd700"/>
    <circle cx="60" cy="20" r="6" fill="#00ff88"/>
    
    <!-- Address bar -->
    <rect x="100" y="10" width="300" height="20" rx="10" fill="#1a1a1a"/>
    <text x="250" y="23" text-anchor="middle" fill="#00d4ff" font-family="Inter, sans-serif" font-size="11">https://example.com</text>
    
    <!-- Webpage content -->
    <rect x="20" y="60" width="460" height="220" rx="5" fill="#1a1a1a"/>
    
    <!-- Simulated webpage elements -->
    <rect x="40" y="80" width="200" height="20" rx="5" fill="#00d4ff" opacity="0.6"/>
    <rect x="40" y="110" width="420" height="10" rx="2" fill="#666"/>
    <rect x="40" y="130" width="350" height="10" rx="2" fill="#666"/>
    <rect x="40" y="150" width="300" height="10" rx="2" fill="#666"/>
    
    <!-- Highlighted elements (AI detection) -->
    <rect x="38" y="78" width="204" height="24" rx="7" fill="none" stroke="#00ff88" stroke-width="2" stroke-dasharray="5,5"/>
    <rect x="260" y="78" width="80" height="24" rx="7" fill="none" stroke="#ffd700" stroke-width="2" stroke-dasharray="5,5"/>
    
    <!-- Form elements -->
    <rect x="40" y="190" width="150" height="25" rx="5" fill="#333" stroke="#00d4ff" stroke-width="1"/>
    <rect x="200" y="190" width="80" height="25" rx="5" fill="#00d4ff" opacity="0.8"/>
    <text x="240" y="205" text-anchor="middle" fill="white" font-family="Inter, sans-serif" font-size="11">Submit</text>
  </g>
  
  <!-- AI Vision/Processing -->
  <g transform="translate(100, 200)">
    <!-- Eye/Vision symbol -->
    <ellipse cx="0" cy="0" rx="25" ry="15" fill="none" stroke="#00ff88" stroke-width="3"/>
    <circle cx="0" cy="0" r="8" fill="#00ff88"/>
    <circle cx="0" cy="0" r="3" fill="white"/>
    
    <!-- Vision rays -->
    <line x1="25" y1="-10" x2="40" y2="-15" stroke="#00ff88" stroke-width="2" opacity="0.6"/>
    <line x1="25" y1="0" x2="40" y2="0" stroke="#00ff88" stroke-width="2" opacity="0.8"/>
    <line x1="25" y1="10" x2="40" y2="15" stroke="#00ff88" stroke-width="2" opacity="0.6"/>
    
    <text x="0" y="40" text-anchor="middle" fill="#00ff88" font-family="Inter, sans-serif" font-size="12" font-weight="600">Seraphine Vision</text>
  </g>
  
  <!-- Playwright automation -->
  <g transform="translate(680, 180)">
    <!-- Automation cursor -->
    <path d="M 0 0 L 0 20 L 5 15 L 10 20 L 15 15 L 8 8 L 15 0 Z" fill="#ffd700"/>
    
    <!-- Movement trail -->
    <path d="M -20 -20 Q -10 -15 0 0 Q 10 5 20 -10" stroke="#ffd700" stroke-width="2" fill="none" opacity="0.6" stroke-dasharray="3,3"/>
    
    <text x="0" y="40" text-anchor="middle" fill="#ffd700" font-family="Inter, sans-serif" font-size="12" font-weight="600">Playwright</text>
  </g>
  
  <!-- Command interpretation -->
  <g transform="translate(50, 50)">
    <rect x="0" y="0" width="200" height="30" rx="15" fill="#333" stroke="#ff6b6b" stroke-width="1"/>
    <text x="100" y="20" text-anchor="middle" fill="#ff6b6b" font-family="Inter, sans-serif" font-size="12">"Click the submit button"</text>
  </g>
  
  <!-- Project title -->
  <text x="400" y="30" text-anchor="middle" fill="url(#browser)" font-family="Inter, sans-serif" font-size="32" font-weight="700">
    Browser Agent
  </text>
  
  <!-- Tech stack -->
  <rect x="50" y="350" width="55" height="25" rx="12" fill="#00ff88" opacity="0.2"/>
  <text x="77" y="367" text-anchor="middle" fill="#00ff88" font-family="Inter, sans-serif" font-size="11" font-weight="500">Gemini</text>
  
  <rect x="115" y="350" width="70" height="25" rx="12" fill="#ffd700" opacity="0.2"/>
  <text x="150" y="367" text-anchor="middle" fill="#ffd700" font-family="Inter, sans-serif" font-size="11" font-weight="500">Playwright</text>
  
  <rect x="195" y="350" width="45" height="25" rx="12" fill="#ff6b6b" opacity="0.2"/>
  <text x="217" y="367" text-anchor="middle" fill="#ff6b6b" font-family="Inter, sans-serif" font-size="11" font-weight="500">MCP</text>
  
  <rect x="250" y="350" width="80" height="25" rx="12" fill="#00d4ff" opacity="0.2"/>
  <text x="290" y="367" text-anchor="middle" fill="#00d4ff" font-family="Inter, sans-serif" font-size="11" font-weight="500">Computer Vision</text>
  
  <!-- Action indicators -->
  <g transform="translate(600, 350)">
    <circle cx="0" cy="0" r="8" fill="#00d4ff" opacity="0.8"/>
    <text x="0" y="3" text-anchor="middle" fill="white" font-family="Inter, sans-serif" font-size="10">1</text>
    <text x="0" y="20" text-anchor="middle" fill="#00d4ff" font-family="Inter, sans-serif" font-size="8">See</text>
    
    <circle cx="30" cy="0" r="8" fill="#00ff88" opacity="0.8"/>
    <text x="30" y="3" text-anchor="middle" fill="white" font-family="Inter, sans-serif" font-size="10">2</text>
    <text x="30" y="20" text-anchor="middle" fill="#00ff88" font-family="Inter, sans-serif" font-size="8">Think</text>
    
    <circle cx="60" cy="0" r="8" fill="#ffd700" opacity="0.8"/>
    <text x="60" y="3" text-anchor="middle" fill="white" font-family="Inter, sans-serif" font-size="10">3</text>
    <text x="60" y="20" text-anchor="middle" fill="#ffd700" font-family="Inter, sans-serif" font-size="8">Act</text>
  </g>
</svg>
